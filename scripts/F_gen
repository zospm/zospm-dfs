#!/bin/sh
#set -x
HLQ=ZOSPMT
MLQ=DFSF20

maxwait=10
jobs='IV3F101J:0 IV3F102J:0 IV3F103J:0 IV3F104J:0 IV3F106J:1 IV3F201J:0 IV3F204J:0 IV3F205J:0 IV3F206J:0 IV3F207J:0 IV3F209J:0 IV3F210J:0 IV3F211J:0 IV3F212J:0 IV3F301J:0 IV3F302J:0 IV3F303J:0 IV3F305J:0 IV3F401J'
ds="${HLQ}.${MLQ}.INSTALIB"
./runjobs "${ds}" ${maxwait} ${jobs}
rc=$?

exit $rc 
